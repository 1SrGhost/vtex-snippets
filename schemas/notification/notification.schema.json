{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "properties": {
      "title": {
        "type": "string",
        "description": "The title of the block see in Site Editor"
      },
      "props": {
        "type": "object",
        "description": "The props of the block",
        "properties": {
          "blockClass": { "$ref": "../blockClass.json" },
          "content": {
            "type": "string",
            "description": "Text to be used in the bar.",
            "default": ""
          }
        }
      }
    },
    "required": ["title"]
  }
  