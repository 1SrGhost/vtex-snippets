{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "title": {
      "type": "string",
      "description": "The title of the block see in Site Editor"
    },
    "props": {
      "type": "object",
      "description": "The props of the block",
      "properties": {
        "width": {
          "type": "number",
          "description": "Product image width (in pixels).",
          "default": 96
        }
      }
    }
  },
  "required": ["title"]
}
